<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <context-param>
    <param-name>webAppRootKey</param-name>
    <param-value>ciki.root</param-value>
  </context-param>
  <welcome-file-list>
    <welcome-file>/</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>CikiServlet</display-name>
    <servlet-name>CikiServlet</servlet-name>
    <servlet-class>com.chulung.ciki.servlet.CikiServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CikiServlet</servlet-name>
    <url-pattern>/CikiServlet</url-pattern>
  </servlet-mapping>
</web-app>